javascript

function isAllowedDirectory(dirPath) {
    const blockedDirectories = ["blocked_folder_1", "blocked_folder_2"]; // Add blocked directory names here
    return !blockedDirectories.some((blockedDir) => dirPath.endsWith(blockedDir));
}

